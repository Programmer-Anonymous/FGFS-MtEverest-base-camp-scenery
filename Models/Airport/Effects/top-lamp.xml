<?xml version="1.0"?>
<PropertyList>
	<path>top-lamp.ac</path>
	<animation>
		<type>noshadow</type>
		<object-name>top-lamp</object-name>
		<object-name>top-lamp-emissive</object-name>
	</animation>
	<animation>
		<type>alpha-test</type>
		<alpha-factor>0.01</alpha-factor>
	</animation>

	<animation>
		<type>timed</type>
		<object-name>top-lamp-emissive</object-name>
		<object-name>top-lamp</object-name>
		<use-personality type="bool">true</use-personality>
		<branch-duration-sec>
			<random>
				<min>0.5</min>
				<max>0.5</max>
			</random>
		</branch-duration-sec>
		<branch-duration-sec>
			<random>
				<min>0.5</min>
				<max>0.5</max>
			</random>
		</branch-duration-sec>
	</animation>
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>10000</max-m>
	</animation>
	<animation>
		<type>billboard</type>
		<object-name>top-lamp-emissive</object-name>
		<spherical type="bool">true</spherical>
	</animation>
	<animation>
		<type>dist-scale</type>
		<object-name>top-lamp-emissive</object-name>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0.2</dep>
			</entry>
			<entry>
				<ind>300</ind>
				<dep>0.4</dep>
			</entry>
			<entry>
				<ind>1500</ind>
				<dep>1</dep>
			</entry>
			<entry>
				<ind>3000</ind>
				<dep>3</dep>
			</entry>
			<entry>
				<ind>6000</ind>
				<dep>5</dep>
			</entry>
		</interpolation>
	</animation>
</PropertyList>
